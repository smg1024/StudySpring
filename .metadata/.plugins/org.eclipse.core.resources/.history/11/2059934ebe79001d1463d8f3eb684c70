<mapper namespace="com.poby.myapp.dao.MemberDAO">
	<select id="idCheck" resultType="int">
		SELECT count(username) cnt FROM member_tbl WHERE username=#{param1}
	</select>
</mapper>

<mapper namespace="com.poby.myapp.dao.ZipcodeVO">
	<select id="searchZipcode" resultType="">
		SELECT zipcode, city, street, bldg1, bldg2, bldgname, dong, num1, num2 FROM zipcode_tbl WHERE street LIKE '%${param1}%';
	</select>
</mapper>